#:kivy 1.0.9

<WorldMap>:

    canvas:
		Rectangle:
            source: 'images/grass.png'
            size: root.size
	Label:
        font_size: 30  
        center_x: root.width / 2
        top: root.top - 5
        text: str(root.ressources)
		
<Castle>:
	canvas:
		Rectangle:
            size: root.size
			pos: (self.x, self.y)